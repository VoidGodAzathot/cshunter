{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cshunter",
  "version": "0.0.0",
  "identifier": "ru.cshunter.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "cshunter",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "capabilities": ["main"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.ico"
    ]
  }
}
